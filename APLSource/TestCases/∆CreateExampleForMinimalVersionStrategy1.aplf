     (⊂body)F.NPUT ∆BuildSrc,'/Main.apln'
     cfg←P.InitPackageConfig ⍬
     cfg.group←'minimalversion'
     cfg.name←'A'
     cfg.version←'1.0.0'
     cfg.source←'Main.apln'
     cfg P.WritePackageConfigFile ∆BuildSrc
     ∆BuildSrc P.Dependencies.Add'minimalversion-B-1.1.2'
     ∆BuildSrc P.Dependencies.Add'minimalversion-C-1.1.2'
     P.PublishPackage ∆BuildSrc ∆BuildReg
     Assert 0=1⊃F.RmDir ∆BuildSrc
     Assert F.MkDir ∆BuildSrc
 :EndIf

