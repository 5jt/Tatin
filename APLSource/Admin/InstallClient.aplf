 ref.ListPackages←##.Client.ListPackages
 ref.LoadDependencies←##.Client.LoadDependencies
 ref.LoadPackage←##.Client.LoadPackage
 ref.Pack←##.Client.Pack
 ref.PublishPackage←##.Client.PublishPackage
 ref.ReadConfig←##.Client.ReadPackageConfigFile
 ref.DefineRegistry←##.Client.DefineRegistry
 ref.UserSettings←##.Client.UserSettings
 ref.ListRegistries←##.Client.ListRegistries
 ref.ListVersions←##.Client.ListVersions
 ref.HelpOnSyntax←##.Client.HelpOnSyntax
 ref.Version←##.Client.Version

 ref.⎕FX'r←Public'('r←',⊃{⍺,' ',⍵}/{'''',⍵,''''}¨' '~¨⍨↓ref.⎕NL 3)
