(
 'Examples:'
 '  ]Tatin.DeletePackages https:/tatin.dev/group-name-1.0.0     ⍝ Registry URL & package ID'
 '  ]Tatin.DeletePackages [test-tatin]group-name-1.0.0          ⍝ Registry alias & package ID'
 '  ]Tatin.DeletePackages [test-tatin]name-1.0.0                ⍝ No group name'
 '  ]Tatin.DeletePackages [test-tatin]name-1                    ⍝ All versions of that package with major=1'
 '  ]Tatin.DeletePackages [test-tatin]name                      ⍝ All versions of that package'
 '  ]Tatin.DeletePackages file:///path/2/Registry/grp-foo-1.0.0 ⍝ Local package'
)
