 {r}←EstablishSystemVars(path cfg);parent;new;ref
 r←⍬
 new←A.Last path
 parent←⍎(-1+≢new)↓path
 new parent.⎕NS''
 ref←⍎path
 :If 2=cfg.⎕NC'io'
 :AndIf ¯1≠cfg.io
     ref.⎕IO←cfg.io
 :EndIf
 :If 2=cfg.⎕NC'ml'
 :AndIf ¯1≠cfg.ml
     ref.⎕ML←cfg.ml
 :EndIf
 :If 2=cfg.⎕NC'wx'
 :AndIf ¯1≠cfg.wx
     ref.⎕WX←cfg.wx
 :EndIf
⍝Done
